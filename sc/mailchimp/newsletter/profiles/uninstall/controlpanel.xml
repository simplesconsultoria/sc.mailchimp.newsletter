<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="sc.mailchimp.newsletter" purge="True">
  <configlet title="MailChimp" action_id="mchimp"
     appId="sc.mailchimp.newsletter" category="Products" condition_expr=""
     url_expr="string:${portal_url}/@@mc-settings"
     icon_expr="string:${portal_url}/++resource++mc_images/ico-mchimp.png"
     visible="False" i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
